// 1.2 - 217
#include <stdio.h>
#define O(x)printf(#x)
int main(){
    int X,Y,x,y;
    scanf("%d%d%d%d",&X,&Y,&x,&y);
    X-=x;
    Y-=y;
    for(;;)
        Y>0?O(S),Y--:0,
        X>0?O(E),X--:(X<0?O(W),X++:0),
        O(\n);
}

/* 164
#include <stdio.h>
#define O(x)printf(#x)
int main(){int X,Y,x,y;scanf("%d%d%d%d",&X,&Y,&x,&y);X-=x;Y-=y;for(;;)Y>0?O(S),Y--:0,X>0?O(E),X--:(X<0?O(W),X++:0),O(\n);}
*/

// 1.3 - 197
#include <stdio.h>
#define O(x)printf(#x)
int main(){
    int X,Y,x,y;
    scanf("%d%d%d%d",&X,&Y,&x,&y);
    for(;;)
        Y>y?O(S),Y--:0,
        X>x?O(E),X--:(X<x?O(W),X++:0),
        O(\n);
}

