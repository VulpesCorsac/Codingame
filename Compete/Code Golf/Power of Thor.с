// 1 - 244
#include <stdio.h>
int main(){
    int X,Y,x,y;
    scanf("%d%d%d%d",&X,&Y,&x,&y);
    X-=x;
    Y-=y;
    for(;;)
        Y>0?printf("S"),Y--:(Y<0?printf("N"),Y++:0),
        X>0?printf("E"),X--:(X<0?printf("W"),X++:0),
        printf("\n");
}

// 1.1 - 222
#include <stdio.h>
int main(){
    int X,Y,x,y;
    scanf("%d%d%d%d",&X,&Y,&x,&y);
    X-=x;
    Y-=y;
    for(;;)
        Y>0?printf("S"),Y--:0,
        X>0?printf("E"),X--:(X<0?printf("W"),X++:0),
        printf("\n");
}

